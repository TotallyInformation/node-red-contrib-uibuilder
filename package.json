{
  "name": "node-red-contrib-uibuilder",
  "version": "5.1.1",
  "description": "Easily create data-driven web UI's for Node-RED using any (or no) front-end library.",
  "scripts": {
    "preinstall": "node ./bin/uibpreinstalljs",
    "build": "gulp",
    "buildfe": "gulp packfe",
    "watch": "gulp watch",
