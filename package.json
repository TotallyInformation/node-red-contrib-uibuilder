    "express-session": "^1.17.2",
    "express-validator": "^6.13.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.1.30",
    "serve-index": "^1.9.1",
    "socket.io": "^4.3.2",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jquery": "^3.5.8",
    "@types/node": "^12.20.47",
    "@types/node-red": "^1.1.1",
    "eslint": "^8.2.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-jsdoc": "^39.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "gulp": "^4.0.2",
    "gulp-debug": "^4.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-include": "^2.4.1",
    "gulp-json-editor": "^2.5.6",
    "gulp-once": "^2.1.1",
    "gulp-prompt": "^1.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-uglify": "^3.0.2"
  },
  "node-red": {
    "version": ">=1.3.0",
    "nodes": {
      "uibuilder": "nodes/uibuilder.js",
      "uib-sender": "nodes/uib-sender.js",
      "uib-cache": "nodes/uib-cache.js"
    },
    "notyetnodes": {
      "uib-receiver": "nodes/uib-receiver.js"
    }
  },
  "engines": {
    "node": ">=12.20"
  },
  "directories": {
    "example": "./examples",
    "doc": "./docs",
    "lib": "./nodes",
    "test": "./tests"
  },
  "main": "./nodes/uibuilder.js",
  "browser": "./front-end/uibuilderfe.min.js",
  "style": "./front-end/uib-styles.css",
  "types": "index.d.ts",
  "type": "commonjs",
  "browserslist": [
    "> 0.5%",
    "maintained node versions",
    "last 2 versions",
    "not dead",
    "not ie > 0"
  ],
  "keywords": [
